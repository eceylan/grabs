.retina-check
    width 400px
    margin 50px auto 0 auto

    &:before
        content ""
        display inline-block
        position relative
        top 8px
        left -8px
        getSprite $check $check-2x

.retina-delete
    width 400px
    margin 0 auto

    &:before
        content ""
        display inline-block
        position relative
        top 8px
        left -8px
        getSprite $delete $delete-2x
